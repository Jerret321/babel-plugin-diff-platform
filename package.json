{
  "name": "babel-plugin-diff-platform",
  "version": "1.0.2",
  "description": "babel plugin diff platform find path",
  "main": "lib/index.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jerret321/babel-plugin-diff-platform.git"
  },
  "keywords": [
    "babel",
    "plugin",
    "diff-platform"
  ],
  "author": "321jiangtao@gmail.com",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2015-loose": "6.x.x",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-1": "6.x.x",
    "babel-runtime": "6.x.x",
    "babel-types": "6.x.x",
    "codecov": "^2.1.0",
    "istanbul": "^0.4.5",
    "nyc": "^10.2.0",
    "vt-alert": "^2.0.0"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 60,
    "extension": [
      ".js",
      ".vue"
    ],
    "cache": true,
    "check-coverage": true,
    "report-dir": "./coverage",
    "exclude": [
      "lib"
    ]
  },
  "scripts": {
    "test": "ava",
    "report": "nyc npm test",
    "build": "babel --presets es2015-loose,stage-1 --plugins transform-runtime src/ --out-dir lib/",
    "watch": "npm run build -- -watch",
    "prepublish": "npm run report && npm run build"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jerret321/babel-plugin-diff-platform/issues"
  },
  "homepage": "https://github.com/Jerret321/babel-plugin-diff-platform#readme"
}
